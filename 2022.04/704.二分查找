//最原始的二分查找
class Solution {
public:
    int search(vector<int>& nums, int target) {
        int begin = 0, end = nums.size() - 1;
        int middle = (begin + end) / 2;
        while (begin <= end) {
            if (nums[middle] == target) return middle;
            else if (nums[middle] < target)
                begin = middle + 1;
            else
                end = middle - 1;
            middle = (begin + end) / 2;
        }
        return -1;
    }
};
